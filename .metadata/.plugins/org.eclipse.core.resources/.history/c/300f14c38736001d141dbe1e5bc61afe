
class Overload{
	public void run() {
		System.out.println("first run method");
	}
	public void run(int i) {
		System.out.println("first run method");
	}
	public void run(String name) {
		System.out.println("first run method");
	}
}
public class Polymorphism {
	public static void main(String[] args) {
		
	}
}
