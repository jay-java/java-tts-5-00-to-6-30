import java.util.HashMap;
import java.util.Map;

public class MapDemo {
	public static void main(String[] args) {
		Map<Integer, String> map = new HashMap<Integer, String>();
		map.put(1, "c");//entruy
		map.put(2,"c++");
		map.put(3, "java");
		map.put(null, "python");
		map.put(4, null);
		map.put(5, null);
		System.out.println(map);
	}
}
