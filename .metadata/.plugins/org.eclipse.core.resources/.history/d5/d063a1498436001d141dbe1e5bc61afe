class Emp{
	int id;
	String ename;
	static String cname ="tops";
	Emp(int id, String ename){
		this.id=id;
		this.ename=ename;
	}
	public void show() {
		System.out.println("id  : "+id+" ename : "+ename+" cname : "+cname);
	}
}
public class StaticKeyword {
	public static void main(String[] args) {
		Emp e1 = new Emp();
	}
}
