
class EmpData{
	int id;
	String name;
	double salary;
	EmpData(int id,String name,double salary){
		this.id=id;
		this.name=name;
		this.salary=salary;
	}
	@Override
	public String toString() {
		return "id : "+id+" name : "+name+" salary : "+salary;
	}
}
public class ListUse {
	public static void main(String[] args) {
		
	}
}
